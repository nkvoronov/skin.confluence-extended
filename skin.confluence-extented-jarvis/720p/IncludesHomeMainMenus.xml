    <include name="HomeMainMenuContentItem">
        <item id="$PARAM[id]">
            <description>Home Menu Item</description>
            <label>$INFO[Skin.String(HomeItem.$PARAM[id].Label)]</label>
            <onclick>SetProperty(MenuItemActive,Item$PARAM[id2],Home)</onclick>
            <onclick>SetProperty(MenuItemSelected,$INFO[Skin.String(HomeItem.$PARAM[id].Type)],Home)</onclick>
            <onclick>SetProperty(MenuBackground,$VAR[Background$PARAM[id2]],Home)</onclick>
            <onclick>$INFO[Skin.String(HomeItem.$PARAM[id].Path)]</onclick>
            <icon>$INFO[Skin.String(HomeItem.$PARAM[id].DBackground)]</icon>
            <thumb>$INFO[Skin.String(HomeItem.$PARAM[id].CBackground)]</thumb>
            <property name="SubMenu">$INFO[Skin.String(HomeItem.$PARAM[id].Sub)]</property>
            <property name="TypeItem">$INFO[Skin.String(HomeItem.$PARAM[id].Type)]</property>
            <property name="AddOnSet">$INFO[Skin.String(HomeItem.$PARAM[id].Addon)]</property>
            <property name="WidgetSet">$INFO[Skin.String(HomeItem.$PARAM[id].Widget)]</property>
            <property name="PlayListType">$INFO[Window(Home).Property(HomePlaylist$PARAM[id2].Type)]</property>
            <property name="Background">$VAR[Background$PARAM[id2]]</property>
            <visible>!IsEmpty(Skin.String(HomeItem.$PARAM[id].Path))</visible>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),Weather)">VisibleWeather</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),Radio)">VisibleRadio</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),LiveTV)">VisibleLiveTV</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),Movies)">VisibleMovies</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),TVShows)">VisibleTVShows</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),MusicVideo)">VisibleMusicVideo</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),Music)">VisibleMusic</include>
            <include condition="stringcompare(Skin.String(HomeItem.$PARAM[id].Type),PlayDisc)">VisiblePlayDisc</include>
        </item>
    </include>
